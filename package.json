{
  "name": "crwn-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "nodemon": "^3.0.3",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
